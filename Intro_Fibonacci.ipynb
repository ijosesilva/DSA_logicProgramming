   "source": [
    "def Fibonaccis_Position(n):\n",
    "    if n < 0:\n",
    "        print(\"Invalid position\")\n",
    "    elif n == 1:\n",
    "        return 0\n",
    "    elif n == 2:\n",
    "        return 1\n",
    "    else:\n",
    "        return Fibonaccis_Position(n-1) + Fibonaccis_Position(n-2)"
   ]
  },
